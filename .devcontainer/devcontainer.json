{
   "name": "java-dev",
   "dockerComposeFile": [
       "docker-compose.yml"
   ],
   "service": "java-dev",
   "workspaceFolder": "/src",
   "extensions": [
       "eamodio.gitlens",
       "editorconfig.editorconfig",
       "shd101wyy.markdown-preview-enhanced",
       "vscjava.vscode-java-pack"
   ],
   "remoteUser": "dev",
   "shutdownAction": "stopCompose"
}
